program WelcomePlugin;

uses
  Windows, Messages, SysUtils, Classes, Graphics, Controls, Forms, Dialogs,
  ActnList, Buttons, ClipBrd, ComCtrls, ExtCtrls, ComObj, ExtDlgs, IniFiles,
  Menus, Printers, Registry, StdCtrls, TypInfo, ToolsAPI, CnDebug,
  AsRegExpr, ScriptEvent, CnCommon, CnWizClasses, CnWizUtils, CnWizIdeUtils,
  CnWizShortCut, CnWizOptions;

  // Only for D12 or above

var
  I: Integer;
  F: TObject;
begin
  for I := 0 to WelcomePagePluginService.GetPluginCount - 1 do
  begin
    Writeln('#' + IntToStr(I) + ' ' + WelcomePagePluginService.GetPluginID(I));
    F := WelcomePagePluginService.GetPluginView(WelcomePagePluginService.GetPluginID(I));
    if F <> nil then
      Writeln('#' + IntToStr(I) + ' ' + F.ClassName)
    else
      Writeln('#' + IntToStr(I) + ' * No View');
  end;
end.
 
